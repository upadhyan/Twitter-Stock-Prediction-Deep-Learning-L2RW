etable)\n","        text_tokens = word_tokenize(text)\n","        tokens_without_sw= [lemmatizer.lemmatize(word) for word in text_tokens if not word in all_stopwords]\n","        token_list = token_list + tokens_without_sw\n","      tweets.close();\n","      json_object = {\n","          'text': token_list,\n","          'label': int(aapl_prices.Close.iloc[i])\n","      }\n","      json_list.append(json_object)\n","  return json_list\n","\n","def messy_stock_process(ticker):\n","  json_list = []\n","  aapl_prices = pd.read_csv('price/raw/'+ticker+'.csv')\n","  labels = aapl_prices['Date']\n","  aapl_prices = aapl_prices.drop(['Date'], axis = 1)\n","  aapl_prices = aapl_prices.diff(periods=-1) * -1\n","  aapl_prices['Date'] = labels\n","  aapl_prices = aapl_prices.drop(['Open', 'High','Low', 'Adj Close', 'Volume'], axis = 1)\n","  #aapl_prices['Previous'] = aapl_prices['Close'].shift(1)\n","  aapl_prices.drop(aapl_prices.tail(1).index,inplace=True)\n","  aapl_prices['Close'] = zscore(aapl_prices['Close'])\n","  aapl_prices['Close'] = aapl_prices['Close'].apply(discretize_prices)\n","  #aapl_prices['Previous'] = aapl_prices['Previous'].apply(discretize_prices)\n","  count = 0\n","  N = len(aapl_prices)\n","  for i in range(len(aapl_prices)):\n","    date = aapl_prices.Date.iloc[i]\n","    file_path = 'tweet/preprocessed/' + ticker + '/' + date\n","    if os.path.isfile(file_path):\n","      tweets = open(file_path)\n","      token_list = []\n","      for line in tweets:\n","        json_val = json.loads(line)\n","        token_list = token_list + json_val['text']\n","      tweets.close();\n","      json_object = {\n","          'text': token_list,\n","          'label': int(aapl_prices.Close.iloc[i])\n","      }\n","      json_list.append(json_object)\n","  return json_list"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"PEBbZZfkVRe-"},"source":["\n","def write_to_files(json_data, percent, clean = True):\n","  fraction = percent / 100\n","  copy = json_data\n","  random.shuffle(copy)\n","  copy = copy[0:int(len(copy) * fraction)]\n","  num_train = int(len(copy) * .7)\n","  train_set = copy[0:num_train]\n","  test_set = copy[(num_train):(len(copy)-1)]\n","  if clean:\n","    train_name = 'train_set_' + str(percent) + \".json\"\n","    test_name = 'test_set_'+ str(percent) +\".json\" \n","  else:\n","    train_name = 'messy_train_set_' + str(percent) + \".json\"\n","    test_name = 'messy_test_set_'+ str(percent) +\".json\" \n","  with open(train_name, 'w+') as openfile:\n","    openfile.write('\\n'.join(json.dumps(i) for i in train_set))\n","  with open(test_name, 'w+') as openfile:\n","    openfile.write('\\n'.join(json.dumps(i) for i in test_set))"],"execution_count":null,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"0fSIakZqzgCM"},"source":["Messy Data Set"]},{"cell_type":"code","metadata":{"id":"XGoILqOnzeTP"},"source":["def write_messy_to_files():\n","  tickers = os.listdir(\"tweet/preprocessed\")\n","  messy_json_vals = []\n","  for i in tqdm(range(len(tickers))):\n","    ticker = tickers[i]\n","    print(\"Current Ticker: \", ticker)\n","    processed = messy_stock_process(ticker)\n","    if processed:\n","      messy_json_vals = messy_json_vals + processed\n","      print(\"Success! There are now \", len(messy_json_vals), \" points\")\n","  write_to_files(messy_json_vals, 100, clean = False)\n","def write_clean_to_files():\n","  tickers = os.listdir(\"tweet/preprocessed\")\n","  end_json_vals = []\n","  for i in tqdm(range(len(tickers))):\n","    ticker = tickers[i]\n","    print(\"Current Ticker: \", ticker)\n","    processed = stock_process(ticker)\n","    if processed:\n","      end_json_vals = end_json_vals + processed\n","      print(\"Success! There are now \", len(end_json_vals), \" points\")\n","  percent = [25,40,55,70,85,100]\n","  for perc in percent:\n","    write_to_files(end_json_vals, perc)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"Wr